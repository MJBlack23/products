  "main": "index.js",
  "scripts": {
    "cleanup": "rm -rf .cache dist",
    "dev:server": "ts-node src/index.ts",
    "dev:ui": "parcel watch src/app/index.tsx --out-dir public/js --out-file app.js",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:ui\"",
