    "build:ui": "parcel build src/app/index.tsx --out-dir public/js --out-file app.js",
    "build": "concurrently \"npm:build:server\" \"npm:build:ui\"",
    "start": "node src/index.js",
    "test": "mocha -r ts-node/register ./src/**/*.test.ts"
  },
